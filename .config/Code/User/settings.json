{
  "workbench.startupEditor": "none",
  "editor.fontFamily": "CaskaydiaCove Nerd Font",
  "terminal.integrated.fontFamily": "FiraMono Nerd Font",
  "editor.fontLigatures": true,
  "editor.quickSuggestions": {
    "comments": true,
    "strings": "on"
  },
  "prettier.useTabs": true,
  "workbench.iconTheme": "catppuccin-mocha",
  "workbench.tree.renderIndentGuides": "none",
  "window.zoomLevel": 0,
  "files.associations": {
    "*.html": "html",
    "*.mcmeta": "json",
    "*.js": "javascriptreact"
  },
  "explorer.confirmDragAndDrop": false,
  "editor.cursorWidth": 3,
  "editor.cursorBlinking": "smooth",
  "editor.fontSize": 16,
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "terminal.integrated.enablePersistentSessions": false,
  "security.workspace.trust.untrustedFiles": "open",
  "editor.formatOnSave": true,
  "workbench.colorTheme": "Catppuccin Mocha",
  "extensions.autoCheckUpdates": false,
  "editor.minimap.renderCharacters": false,
  // "tailwindCSS.experimental.configFile": null,
  // "tailwindCSS.emmetCompletions": true,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "window.commandCenter": false,
  "editor.codeActionsOnSave": {},
  // "tailwindCSS.includeLanguages": {
  // 	"Svelte": "html"
  // },
  "workbench.colorCustomizations": {},
  "editor.lineHeight": 2,
  "editor.stickyScroll.enabled": false,
  "window.dialogStyle": "custom",
  // "editor.scrollbar.vertical": "visible",
  "editor.scrollbar.verticalScrollbarSize": 0,
  "explorer.compactFolders": false,
  "editor.bracketPairColorization.enabled": false,
  "editor.matchBrackets": "never",
  "editor.guides.highlightActiveBracketPair": false,
  "editor.folding": false,
  "terminal.integrated.stickyScroll.enabled": true,
  "editor.wordWrap": "on",
  "editor.scrollbar.horizontalScrollbarSize": 0,
  // "editor.scrollbar.horizontal": "hidden",
  "telemetry.telemetryLevel": "off",
  "workbench.activityBar.location": "hidden",
  "editor.renderLineHighlight": "none",
  "workbench.layoutControl.type": "menu",
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.value": 5,
  "workbench.editor.limit.perEditorGroup": true,
  "workbench.editor.showTabs": "none",
  "files.autoSave": "onWindowChange",
  "explorer.openEditors.visible": 1,
  "workbench.layoutControl.enabled": false,
  "window.titleSeparator": " / ",
  // "window.title": "${dirty}${activeEditorShort}${separator}${rootName}${separator}${profileName}",
  "window.title": "${activeEditorShort} / ${rootName}",
  "breadcrumbs.enabled": false,
  "workbench.tips.enabled": false,
  "workbench.sideBar.location": "right",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.statusBar.visible": false,
  "editor.hover.enabled": false,
  "editor.tabSize": 4,
  "editor.detectIndentation": false,
  "explorer.confirmDelete": false,
  "git.decorations.enabled": false,
  "scm.diffDecorations": "none",
  "extensions.ignoreRecommendations": true,
  "workbench.editor.editorActionsLocation": "hidden",
  "window.titleBarStyle": "native",
  "update.mode": "manual",
  "workbench.tree.enableStickyScroll": false,
  // "vscode_custom_css.imports": [
  // 	"file:///C:/vscode/custom-css-js/custom-style-catppuccin-mocha.css"
  // ],
  "editor.minimap.autohide": true,
  "window.menuBarVisibility": "hidden"
}
