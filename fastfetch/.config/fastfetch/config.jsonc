// Inspired by Catnap
{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "logo": {
        // "source": "~/Pictures/arch-moonbrooke.png",
        "type": "small",
        // "width": 20,
        "padding": {
            "top": 2,
            "left": 2,
        }
    },
    "display": {
        "separator": " "
    },
    "modules": [
        {
            "key": "╭────────────╮",
            "type": "custom",
        },
        {
            "key": "│ {#31} user     {#keys}│",
            "type": "title",
            "format": "{user-name}"
        },
        {
            "key": "│ {#31}󰇅 hostname {#keys}│",
            "type": "title"
        },
        {
            "key": "│ {#31}󰅐 uptime   {#keys}│",
            "type": "uptime"
        },
        {
            "key": "│ {#34}{icon} distro   {#keys}│",
            "type": "os"
        },
        {
            "key": "│ {#32} kernel   {#keys}│",
            "type": "kernel"
        },
        {
            "key": "│ {#32}󰨇 de       {#keys}│",
            "type": "de",
        },
        {
            "key": "│ {#32}󰨇 wm       {#keys}│",
            "type": "wm"
        },
        {
            "key": "│ {#32}󰏓 packages {#keys}│",
            "type": "packages",
        },
        {
            "key": "│ {#33} terminal {#keys}│",
            "type": "terminal"
        },
        {
            "key": "│ {#33} shell    {#keys}│",
            "type": "shell"
        },
        {
            "key": "│ {#35}󰍛 cpu      {#keys}│",
            "type": "cpu",
        },
        {
            "key": "│ {#35} gpu      {#keys}│",
            "type": "gpu",
        },
        {
            "key": "│ {#35} memory   {#keys}│",
            "type": "memory"
        },
        //{
        //    "key": "│ {#36}󰩟 network  {#keys}│",
        //    "type": "localip",
        //    "format": "{ipv4} ({ifname})"
        //},
        {
            "key": "│ {#36}󰍺 display  {#keys}│",
            "type": "display",
        },
        {
            "key": "├────────────┤",
            "type": "custom",
            "format": ""
        },
        {
            "key": "│ {#39} colors   {#keys}│",
            "type": "colors",
            "symbol": "circle"
        },
        {
            "key": "╰────────────╯",
            "type": "custom",
            "format": ""
        }
    ]
}
